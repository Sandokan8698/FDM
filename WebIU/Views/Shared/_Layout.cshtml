
<!DOCTYPE html>
<html lang="en">
<head>
    <title>Boooya - Revolution Admin Template</title>

    <!-- META SECTION -->
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="shortcut icon" href="favicon.ico" type="image/x-icon">
    <link rel="icon" href="favicon.ico" type="image/x-icon">
    <!-- END META SECTION -->
    <!-- CSS INCLUDE -->
    <link rel="stylesheet" href="~/Content/css/styles.css">
    <!-- EOF CSS INCLUDE -->
</head>
<body>

    <!-- APP WRAPPER -->
    <div class="app">

        <!-- START APP CONTAINER -->
        <div class="app-container">
            <!-- START SIDEBAR -->
            <div class="app-sidebar app-navigation  app-navigation-fixed app-navigation-style-default dir-left" data-type="close-other">
                <a href="index.html" class="app-navigation-logo"></a>
                <nav>
                    <ul>
                        <li><a href="#"><span class="icon-home"></span> Navigation Item</a></li>
                        @Html.Action("Menu", "Nav")
                      </ul>
                </nav>
            </div>
            <!-- END SIDEBAR -->
            <!-- START APP CONTENT -->
            <div class="app-content app-sidebar-left">
                <!-- START APP HEADER -->
                <div class="app-header">
                    <ul class="app-header-buttons">
                        <li class="visible-mobile"><a href="#" class="btn btn-link btn-icon" data-sidebar-toggle=".app-sidebar.dir-left"><span class="icon-menu"></span></a></li>
                        <li class="hidden-mobile"><a href="#" class="btn btn-link btn-icon" data-sidebar-minimize=".app-sidebar.dir-left"><span class="icon-list4"></span></a></li>
                    </ul>
                    <form class="app-header-search" action="" method="post">
                        <input type="text" name="keyword" placeholder="Search">
                    </form>
                </div>
                <!-- END APP HEADER  -->
                <!-- START PAGE HEADING -->
                @RenderSection("PageHeading", false)
                <!-- END PAGE HEADING -->
                <!-- START PAGE CONTAINER -->
                <div class="container">
                    
                    @if (ViewBag.ErrorMessage != null)
                    {
                        <div class="row">
                            <div class="col-md-12">
                                <div class="alert alert-danger alert-icon-block alert-dismissible" role="alert"> 
                                    <div class="alert-icon">
                                        <span class="icon-menu-circle"></span> 
                                    </div>                                        
                                    <strong>Erorr!</strong> @ViewBag.ErrorMessage
                                    <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span class="fa fa-times"></span></button>
                                </div>
                            </div>
                        </div>
                    }
   

                    @RenderBody()

                </div>
                <!-- END PAGE CONTAINER -->

            </div>
            <!-- END APP CONTENT -->

        </div>
        <!-- END APP CONTAINER -->

    </div>
    <!-- END APP WRAPPER -->
    <!-- START SCRIPTS -->
    <script type="text/javascript" src="~/Scripts/js/vendor/jquery/jquery.min.js"></script>
    <script type="text/javascript" src="~/Scripts/js/vendor/jquery/jquery-ui.min.js"></script>
    <script type="text/javascript" src="~/Scripts/js/vendor/bootstrap/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/Scripts/js/vendor/moment/moment.min.js"></script>

    <script type="text/javascript" src="~/Scripts/js/vendor/customscrollbar/jquery.mCustomScrollbar.min.js"></script>
    <script type="text/javascript" src="~/Scripts/js/vendor/bootstrap-select/bootstrap-select.js"></script>
    <script type="text/javascript" src="~/Scripts/js/vendor/bootstrap-datetimepicker/bootstrap-datetimepicker.js"></script>

    <script type="text/javascript" src="~/Scripts/js/vendor/maskedinput/jquery.maskedinput.min.js"></script>
    <script type="text/javascript" src="~/Scripts/js/vendor/form-validator/jquery.form-validator.min.js"></script>

    <script type="text/javascript" src="~/Scripts/js/vendor/noty/jquery.noty.packaged.js"></script>

    <script type="text/javascript" src="~/Scripts/js/vendor/datatables/jquery.dataTables.min.js"></script>
    <script type="text/javascript" src="~/Scripts/js/vendor/datatables/dataTables.bootstrap.min.js"></script>

    <script type="text/javascript" src="~/Scripts/js/vendor/sweetalert/sweetalert.min.js"></script>
    <script type="text/javascript" src="~/Scripts/js/vendor/knob/jquery.knob.min.js"></script>

    <script type="text/javascript" src="~/Scripts/js/vendor/jvectormap/jquery-jvectormap.min.js"></script>
    <script type="text/javascript" src="~/Scripts/js/vendor/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <script type="text/javascript" src="~/Scripts/js/vendor/jvectormap/jquery-jvectormap-us-aea-en.js"></script>

    <script type="text/javascript" src="~/Scripts/js/vendor/sparkline/jquery.sparkline.min.js"></script>

    <script type="text/javascript" src="~/Scripts/js/vendor/morris/raphael.min.js"></script>
    <script type="text/javascript" src="~/Scripts/js/vendor/morris/morris.min.js"></script>

    <script type="text/javascript" src="~/Scripts/js/vendor/rickshaw/d3.v3.js"></script>
    <script type="text/javascript" src="~/Scripts/js/vendor/rickshaw/rickshaw.min.js"></script>

    <script type="text/javascript" src="~/Scripts/js/vendor/isotope/isotope.pkgd.min.js"></script>

    <script type="text/javascript" src="~/Scripts/js/app.js"></script>
    <script type="text/javascript" src="~/Scripts/js/app_plugins.js"></script>
    <script type="text/javascript" src="~/Scripts/js/app_demo.js"></script>
    <!-- END SCRIPTS -->
</body>
</html>